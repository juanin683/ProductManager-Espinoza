{{#each allProducts}}

    <ul class="ulImage">
    <img class="card-image"  src="{{this.thumbnail}}" alt="">
    <li class="liImage">
        Id: {{_id}}
    </li class="liImage">
    <li class="liImage">
        Titulo: {{this.title}}
    </li class="liImage">
    <li class="liImage">
        Descripci√≥n: {{this.description}}
    </li class="liImage">
    <li class="liImage">
